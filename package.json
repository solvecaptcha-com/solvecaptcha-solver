{
    "title": "__MSG_extName__",
    "name": "__MSG_extShortName__",
    "version": "1.0.0",
    "description": "__MSG_extDescription__",
    "author": "solvecaptcha",
    "type": "module",
    "scripts": {
        "dev": "vite",
        "build": "run-s clean && tsc && vite build",
        "watch": "run-s clean && tsc && vite build --watch",
        "clean": "run-p clean:*",
        "copy": "rimraf ./dist && cp -r ./extension ./dist",
        "obfuscator": "npm run copy && npx javascript-obfuscator --config obfuscator.conf.json dist --output dist/ --exclude config.js",
        "obfuscator:public": "npm run copy && npx javascript-obfuscator dist --output dist/ --exclude config.js",
        "clean:dist": "rimraf extension/app",
        "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
        "preview": "vite preview",
        "zip": "npm-build-zip",
        "chrome": "npm run build && npm run obfuscator && npm run zip -- --source=dist --destination=web-ext-artifacts --name=solvecaptcha-solver-chrome-$(awk -F\\\" '/\"version\":/ {print $4}' package.json) --name_only=true",
        "firefox": "git archive -o web-ext-artifacts/solvecaptcha-solver-firefox-$(awk -F\\\\\\\" '/\\\"version\\\":/ {print $4}' package.json).zip -9 firefox --worktree-attributes",
        "public": "npm run build && npm run obfuscator:public && npm run zip -- --source=dist --destination=web-ext-artifacts --name=solvecaptcha-solver-chrome-$(awk -F\\\" '/\"version\":/ {print $4}' package.json) --name_only=true"
    },
    "dependencies": {
        "@emotion/react": "^11.9.3",
        "@emotion/styled": "^11.9.3",
        "@mui/icons-material": "^5.14.18",
        "@mui/material": "^5.14.18",
        "@mui/x-data-grid": "^6.18.2",
        "react": "^18.2.0",
        "react-dom": "^18.2.0"
    },
    "devDependencies": {
        "@crxjs/vite-plugin": "^2.0.0-beta.17",
        "@types/chrome": "0.0.35",
        "@types/react": "^18.2.37",
        "@types/react-dom": "^18.2.15",
        "@typescript-eslint/eslint-plugin": "^6.10.0",
        "@typescript-eslint/parser": "^6.10.0",
        "@vitejs/plugin-react": "^4.2.0",
        "copyfiles": "^2.4.1",
        "eslint": "^8.53.0",
        "eslint-plugin-react-hooks": "^4.6.0",
        "eslint-plugin-react-refresh": "^0.4.4",
        "javascript-obfuscator": "^4.0.2",
        "npm-build-zip": "^1.0.4",
        "npm-run-all2": "^6.0.5",
        "rimraf": "^3.0.2",
        "sass": "^1.69.5",
        "typescript": "^5.2.2",
        "vite": "^5.0.0",
        "vite-plugin-css-injected-by-js": "^3.3.0"
    },
    "engines": {
